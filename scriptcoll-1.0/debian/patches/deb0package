--- /dev/null
+++ scriptcoll-1.0/autocp
@@ -0,0 +1,17 @@
+#!/bin/bash
+BASEDIR="/mnt"
+TARGET="SAVES"
+mkdir $TARGET
+PARTITION=$1 # dev/sd[0-9]+
+MOUNTPOINT=$BASEDIR/`echo $PARTITION | egrep -o sd.[0-9]+`
+mkdir -p $MOUNTPOINT
+mount $PARTITION $MOUNTPOINT
+if [ $? -eq 0 ] ; then
+	cp -r $MOUNTPOINT $TARGET
+    umount $PARTITION
+    if [ $? -eq 0 ] ; then
+    	rm -r $MOUNTPOINT
+    fi
+else
+	rm -r $MOUNTPOINT
+fi
--- /dev/null
